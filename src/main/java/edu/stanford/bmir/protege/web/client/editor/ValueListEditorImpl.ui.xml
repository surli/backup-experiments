<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .main table {
            border-collapse: collapse;
            border: none;
            padding: 0;
        }
        .main td {
            border-collapse: collapse;
            border: none;
            padding: 0;
        }
    </ui:style>

    <g:HTMLPanel addStyleNames="{style.main} web-protege-value-list">
        <g:FlexTable ui:field="tableField" width="100%"/>
    </g:HTMLPanel>
</ui:UiBinder>
