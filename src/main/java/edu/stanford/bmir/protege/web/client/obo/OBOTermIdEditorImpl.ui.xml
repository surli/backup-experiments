<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>

    <g:HTMLPanel addStyleNames="{wp.style.formMain}">

        <div class="{wp.style.formGroup}">
            <g:Label text="Id" styleName="{wp.style.formLabel}"/>
            <g:TextBox ui:field="idField" enabled="false" width="100%"/>
        </div>

        <div class="{wp.style.formGroup}">
            <g:Label text="Name" addStyleNames="{wp.style.formLabel}"/>
            <g:TextBox ui:field="nameField" width="100%"/>
        </div>


        <div class="{wp.style.formGroup}">
            <g:Label text="Namespace" addStyleNames="{wp.style.formLabel}"/>
            <g:SuggestBox ui:field="namespaceField" width="100%"/>
        </div>

    </g:HTMLPanel>
</ui:UiBinder>
