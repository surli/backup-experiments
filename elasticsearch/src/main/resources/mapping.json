{
  "properties" : {
    "_ids": {
      "type": "keyword"
    }
  },
  "dynamic_templates": [
    {
      "locationgeo": {
        "match": "_location",
        "match_mapping_type": "string",
        "mapping": {
          "type": "geo_point"
        }
      }
    },
    {
      "boolean_type": {
        "match": "_boolean",
        "mapping": {
          "type": "boolean"
        }
      }
    },
    {
      "date_type": {
        "match": "_date",
        "mapping": {
          "type": "long"
        }
      }
    },
    {
      "number_type": {
        "match": "_number",
        "mapping": {
          "type": "double"
        }
      }
    },
    {
      "shapegeo": {
        "match": "_polygon",
        "match_mapping_type": "object",
        "mapping": {
          "type": "geo_shape"
        }
      }
    },
    {
      "data_template_top": {
        "match": "data",
        "mapping": {
          "type": "{dynamic_type}",
          "include_in_all": false,
          "index": false,
          "ignore_malformed": true
        }
      }
    },
    {
      "data_template": {
        "path_match": "data.*",
        "mapping": {
          "type": "{dynamic_type}",
          "include_in_all": false,
          "index": false,
          "ignore_malformed": true
        }
      }
    },
    {
      "int_template": {
        "match": "_*",
        "match_mapping_type": "string",
        "mapping": {
          "type": "keyword",
          "ignore_above": 1024
        }
      }
    },
    {
      "notanalyzed": {
        "match": "*",
        "match_mapping_type": "string",
        "mapping": {
          "type": "text",
          "fields": {
            "raw": {
              "type": "keyword",
              "ignore_above": 512
            },
            "match": {
              "type": "text",
              "analyzer": "text_analyzer"
            }
          }
        }
      }
    }
  ]
}
