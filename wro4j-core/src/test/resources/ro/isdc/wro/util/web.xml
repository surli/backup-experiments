<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

  <display-name>WebResourceOptimizer Test Project</display-name>

  <!-- Web Resource Optimizer filter and listener -->
  <filter-mapping>
    <filter-name>Test1</filter-name>
    <url-pattern>/wro/*</url-pattern>
  </filter-mapping>

  <filter-mapping>
    <filter-name>Test2</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>

  <filter-mapping>
    <filter-name>Test3</filter-name>
    <url-pattern>*.jsx</url-pattern>
  </filter-mapping>

</web-app>
