        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasTopping"/>
                <owl:someValuesFrom rdf:resource="#CaperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>



