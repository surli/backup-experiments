        <rdfs:subClassOf>
            <owl:Class rdf:about="#PizzaBase"/>
        </rdfs:subClassOf>
    </owl:Class>



    <!-- Class: http://www.co-ode.org/ontologies/pizza/pizza.owl#ThinAndCrispyPizza -->

